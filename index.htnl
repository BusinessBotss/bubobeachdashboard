    <script>
        // Global variables
        let selectedBed = null;
        let currentTime = new Date();
        let currentPage = 'dashboard';

        // Initialize
        document.addEventListener('DOMContentLoaded', function() {
            initializeApp();
        });

        function initializeApp() {
            // Set current date
            const today = new Date().toISOString().split('T')[0];
            const dateFilter = document.getElementById('dateFilter');
            if (dateFilter) {
                dateFilter.value = today;
            }

            // Start clock
            updateClock();
            setInterval(updateClock, 1000);

            // Initialize navigation
            initializeNavigation();

            // Initialize reservations
            initializeReservations();

            // Initialize charts
            initializeCharts();

            // Initialize settings
            initializeSettings();

            // Initialize mobile menu
            initializeMobileMenu();

            // Update counts
            updateCounts();
        }

        // Clock functionality
        function updateClock() {
            currentTime = new Date();
            const timeString = currentTime.toLocaleTimeString('en-US', { 
                hour: '2-digit', 
                minute: '2-digit',
                second: '2-digit',
                hour12: false
            });
            const clockElement = document.getElementById('currentTime');
            if (clockElement) {
                clockElement.textContent = timeString;
            }
        }

        // Navigation
        function initializeNavigation() {
            const navLinks = document.querySelectorAll('.nav-link');
            navLinks.forEach(link => {
                link.addEventListener('click', (e) => {
                    e.preventDefault();
                    const page = link.dataset.page;
                    if (page) {
                        switchPage(page);
                    }
                });
            });
        }

        function switchPage(page) {
            // Update active nav link
            document.querySelectorAll('.nav-link').forEach(link => {
                link.classList.remove('active');
            });
            document.querySelector(`[data-page="${page}"]`).classList.add('active');

            // Hide all pages
            document.querySelectorAll('.page-section').forEach(section => {
                section.classList.remove('active');
            });

            // Show selected page
            document.getElementById(page).classList.add('active');

            // Update page title
            const titles = {
                dashboard: 'Dashboard',
                reservations: 'Reservations',
                analytics: 'Analytics',
                settings: 'Settings'
            };

            const subtitles = {
                dashboard: 'Welcome to BuBo Beach Management',
                reservations: 'Manage bed reservations and floor plan',
                analytics: 'Business insights and performance metrics',
                settings: 'Configure your system preferences'
            };

            document.getElementById('pageTitle').textContent = titles[page];
            document.getElementById('pageSubtitle').textContent = subtitles[page];

            currentPage = page;
        }

        // Mobile menu
        function initializeMobileMenu() {
            const sidebarToggle = document.getElementById('sidebarToggle');
            const sidebar = document.getElementById('sidebar');

            if (sidebarToggle && sidebar) {
                sidebarToggle.addEventListener('click', () => {
                    sidebar.classList.toggle('open');
                });

                // Close sidebar when clicking outside
                document.addEventListener('click', (e) => {
                    if (!sidebar.contains(e.target) && !sidebarToggle.contains(e.target)) {
                        sidebar.classList.remove('open');
                    }
                });
            }
        }

        // Reservations functionality
        function initializeReservations() {
            const daybeds = document.querySelectorAll('.daybed');
            const tooltip = document.getElementById('tooltip');

            daybeds.forEach(bed => {
                bed.addEventListener('mouseenter', (e) => {
                    showTooltip(e, bed);
                });

                bed.addEventListener('mouseleave', () => {
                    hideTooltip();
                });

                bed.addEventListener('click', () => {
                    const status = getStatus(bed);
                    if (status === 'available' || status === 'walk-in') {
                        selectBed(bed);
                    }
                });
            });

            // Filters
            const zoneFilter = document.getElementById('zoneFilter');
            const statusFilter = document.getElementById('statusFilter');

            if (zoneFilter) zoneFilter.addEventListener('change', applyFilters);
            if (statusFilter) statusFilter.addEventListener('change', applyFilters);

            // Reservation form
            const reservationForm = document.getElementById('quickReservationForm');
            if (reservationForm) {
                reservationForm.addEventListener('submit', handleReservationSubmit);
            }
        }

        function showTooltip(e, bed) {
            const tooltip = document.getElementById('tooltip');
            if (!tooltip) return;

            const bedNum = bed.dataset.bed;
            const price = bed.dataset.price;
            const capacity = bed.dataset.capacity;
            const isVip = bed.dataset.vip === 'true';
            const status = getStatus(bed);
            
            let statusText = '';
            let statusClass = '';
            switch(status) {
                case 'available': 
                    statusText = 'Available'; 
                    statusClass = 'badge-success';
                    break;
                case 'reserved': 
                    statusText = 'Reserved'; 
                    statusClass = 'badge-error';
                    break;
                case 'walk-in': 
                    statusText = 'Walk-in Available'; 
                    statusClass = 'badge-info';
                    break;
            }
            
            tooltip.innerHTML = `
                <div class="space-y-2">
                    <div class="flex items-center justify-between">
                        <span class="font-semibold">Bed #${bedNum}</span>
                        ${isVip ? '<span class="badge badge-vip">VIP</span>' : ''}
                    </div>
                    <div class="text-sm opacity-80">
                        <div>Capacity: ${capacity} guests</div>
                        <div>Price: ‚Ç¨${price}/day</div>
                    </div>
                    <div class="flex items-center">
                        <span class="badge ${statusClass}">${statusText}</span>
                    </div>
                    ${status === 'available' ? '<div class="text-xs mt-2 opacity-60">Click to reserve</div>' : ''}
                </div>
            `;
            
            tooltip.style.left = e.pageX + 10 + 'px';
            tooltip.style.top = e.pageY - 10 + 'px';
            tooltip.classList.add('show');
        }

        function hideTooltip() {
            const tooltip = document.getElementById('tooltip');
            if (tooltip) {
                tooltip.classList.remove('show');
            }
        }

        function getStatus(bed) {
            if (bed.classList.contains('reserved')) return 'reserved';
            if (bed.classList.contains('walk-in')) return 'walk-in';
            return 'available';
        }

        function selectBed(bed) {
            // Clear previous selection
            document.querySelectorAll('.daybed').forEach(b => b.classList.remove('selected'));
            
            // Select new bed
            bed.classList.add('selected');
            selectedBed = bed;
            
            showSelectionSummary(bed);
            showReservationForm(bed);
        }

        function showSelectionSummary(bed) {
            const summaryElement = document.getElementById('selectionSummary');
            const summaryContent = document.getElementById('summaryContent');
            
            if (!summaryElement || !summaryContent) return;

            const bedNum = bed.dataset.bed;
            const price = bed.dataset.price;
            const capacity = bed.dataset.capacity;
            const isVip = bed.dataset.vip === 'true';
            const status = getStatus(bed);
            
            summaryContent.innerHTML = `
                <div class="space-y-4">
                    <div class="flex justify-between items-center">
                        <span class="font-semibold">Bed #${bedNum}</span>
                        ${isVip ? '<span class="badge badge-vip">VIP</span>' : ''}
                    </div>
                    <div class="space-y-2 text-sm text-gray-600">
                        <div>Capacity: ${capacity} guests</div>
                        <div>Price: ‚Ç¨${price}/day</div>
                        <div>Status: ${status === 'walk-in' ? 'Walk-in Available' : 'Available'}</div>
                    </div>
                </div>
            `;
            
            summaryElement.classList.remove('hidden');
        }

        function showReservationForm(bed) {
            const formElement = document.getElementById('reservationForm');
            const totalPriceElement = document.getElementById('formTotalPrice');
            
            if (!formElement || !totalPriceElement) return;

            const price = bed.dataset.price;
            totalPriceElement.textContent = `‚Ç¨${price}`;
            formElement.classList.remove('hidden');
        }

        function clearSelection() {
            if (selectedBed) {
                selectedBed.classList.remove('selected');
                selectedBed = null;
            }
            
            const summaryElement = document.getElementById('selectionSummary');
            const formElement = document.getElementById('reservationForm');
            
            if (summaryElement) summaryElement.classList.add('hidden');
            if (formElement) formElement.classList.add('hidden');
        }

        function applyFilters() {
            const zoneFilter = document.getElementById('zoneFilter');
            const statusFilter = document.getElementById('statusFilter');
            
            if (!zoneFilter || !statusFilter) return;

            const zoneValue = zoneFilter.value;
            const statusValue = statusFilter.value;
            
            const daybeds = document.querySelectorAll('.daybed');
            
            daybeds.forEach(bed => {
                let showBed = true;
                
                // Zone filter
                if (zoneValue !== 'all') {
                    const bedZone = bed.dataset.zone;
                    if (bedZone !== zoneValue) {
                        showBed = false;
                    }
                }
                
                // Status filter
                if (statusValue !== 'all') {
                    const bedStatus = getStatus(bed);
                    if (bedStatus !== statusValue) {
                        showBed = false;
                    }
                }
                
                bed.style.display = showBed ? 'flex' : 'none';
            });
            
            updateCounts();
        }

        function updateCounts() {
            const daybeds = document.querySelectorAll('.daybed');
            const visibleBeds = Array.from(daybeds).filter(bed => bed.style.display !== 'none');
            const available = visibleBeds.filter(bed => getStatus(bed) === 'available').length;
            const reserved = visibleBeds.filter(bed => getStatus(bed) === 'reserved').length;
            
            const availableElement = document.getElementById('availableCount');
            const reservedElement = document.getElementById('reservedCount');
            
            if (availableElement) availableElement.textContent = available;
            if (reservedElement) reservedElement.textContent = reserved;
        }

        function handleReservationSubmit(e) {
            e.preventDefault();
            
            if (!selectedBed) return;
            
            const submitBtn = e.target.querySelector('button[type="submit"]');
            const originalText = submitBtn.textContent;
            
            // Show loading state
            submitBtn.innerHTML = '<div class="loading-spinner"></div> Processing...';
            submitBtn.disabled = true;
            
            setTimeout(() => {
                // Show success notification
                showNotification('Reservation Confirmed', 'Customer will receive confirmation via WhatsApp and email.', 'success');
                
                // Mark bed as reserved
                selectedBed.classList.remove('available', 'walk-in');
                selectedBed.classList.add('reserved');
                selectedBed.innerHTML = '<span class="font-bold">' + selectedBed.dataset.bed + '</span><span class="text-xs opacity-80">Reserved</span>';
                
                clearSelection();
                updateCounts();
                
                submitBtn.textContent = originalText;
                submitBtn.disabled = false;
                
                // Reset form
                e.target.reset();
            }, 2000);
        }

