        function generateOccupancyHeatmap() {
            const heatmapContainer = document.getElementById('occupancyHeatmap');
            if (!heatmapContainer) return;

            const days = ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun'];
            const hours = ['9', '11', '13', '15', '17', '19'];
            
            // Generate sample data
            const data = [];
            for (let day = 0; day < 7; day++) {
                for (let hour = 0; hour < 6; hour++) {
                    const occupancy = Math.floor(Math.random() * 100);
                    data.push({
                        day: days[day],
                        hour: hours[hour],
                        occupancy: occupancy
                    });
                }
            }

            heatmapContainer.innerHTML = '';
            
            data.forEach(item => {
                const cell = document.createElement('div');
                cell.className = 'heatmap-cell';
                cell.textContent = item.occupancy + '%';
                
                // Color based on occupancy
                if (item.occupancy < 30) {
                    cell.style.backgroundColor = '#FEF3C7';
                    cell.style.color = '#92400E';
                } else if (item.occupancy < 60) {
                    cell.style.backgroundColor = '#FDE68A';
                    cell.style.color = '#92400E';
                } else if (item.occupancy < 80) {
                    cell.style.backgroundColor = '#F59E0B';
                    cell.style.color = 'white';
                } else {
                    cell.style.backgroundColor = '#D97706';
                    cell.style.color = 'white';
                }
                
                cell.title = `${item.day} ${item.hour}:00 - ${item.occupancy}% occupancy`;
                heatmapContainer.appendChild(cell);
            });
        }

